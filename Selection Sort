class Solution: 
    def select(self, arr, i):
        pass
    def selectionSort(self, arr,n):
        for i in range(len(arr)):
            min = i
            for j in range(i+1, len(arr)):
                if arr[min] > arr[j]:
                    min = j
            arr[i], arr[min] = arr[min], arr[i]
        return arr
